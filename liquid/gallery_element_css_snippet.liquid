{% case size %}
{% when 0 %}
.G{{ index }} {width: {{ gallery.dims[0][0]}}px; height: {{ gallery.dims[0][1]}}px}
{% when 1 %}
{% if gallery.dims[1][0] != gallery.dims[0][0] %}
.G{{ index }} {width: {{ gallery.dims[1][0]}}px; height: {{ gallery.dims[1][1]}}px}
{% endif %}
{% when 2 %}
{% if gallery.dims[2][0] != gallery.dims[1][0] %}
.G{{ index }} {width: {{ gallery.dims[2][0]}}px; height: {{ gallery.dims[2][1]}}px}
{% endif %}
{% when 3 %}
{% if gallery.dims[3][0] != gallery.dims[2][0] %}
.G{{ index }} {width: {{ gallery.dims[3][0]}}px; height: {{ gallery.dims[3][1]}}px}
{% endif %}
{% endcase %}
