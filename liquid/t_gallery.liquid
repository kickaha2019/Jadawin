<DIV CLASS="gallery t1">
  {% for item in gallery.images %}
    <DIV>
      <DIV>
        {% render 't_image',
                  specs:false,
                  details:item.details,
                  id:false,
                  title:false,
                  clazz:item.clazz,
                  overlay:item.overlay %}
      </DIV>
      <DIV>
        <P>{% render 't_content' with content:item.tag %}</P>
      </DIV>
    </DIV>
  {% endfor %}
  {% for i in (1..8) %}
    <DIV CLASS="dummy size1 size2 size3"></DIV>
  {% endfor %}
</DIV>
