{% for element in content %}
    {% case element.type %}
      {% when "code" %}
        {% include 'code_css', code:element, size:size %}
      {% when "css" %}
        {{ element.text }}
      {% when "gallery" %}
        {% include 'gallery_css', gallery:element, dimensions:dimensions, size:size %}
      {% when "heading" %}
        {% include 'heading_css', heading:element %}
      {% when "image" %}
        {% include 'image_css', image:element.details, id:element.id, dimensions:dimensions, size:size %}
      {% when "svg" %}
        {% include 'svg_css', svg:element, dimensions:dimensions, size:size %}
      {% when "table" %}
        {% include 'table_css', table:element, size:size %}
    {% endcase %}
{% endfor %}

